{
  "name": "node-rpi-feeder",
  "version": "1.0.0",
  "description": "Pet feeder with live stream and hotword detection powered by a Raspberry Pi 3 Model B and Node.js",
  "author": "Pedro Rodrigues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pgrodrigues/node-rpi-feeder.git"
  },
  "bugs": {
    "url": "https://github.com/pgrodrigues/node-rpi-feeder/issues"
  },
  "keywords": [
    "nodejs",
    "raspberry-pi",
    "express",
    "socket-io",
    "javascript"
  ],
  "scripts": {
    "docs": "./node_modules/.bin/jsdoc -c ./.jsdocrc -R ./README.md",
    "lint": "./node_modules/.bin/eslint ./",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run lint & node ./bin/www",
    "start-prod": "cross-env NODE_ENV=production node ./bin/www"
  },
  "dependencies": {
    "@google-cloud/speech": "~3.6.0",
    "compression": "~1.7.4",
    "config": "~3.3.1",
    "cookie-parser": "~1.4.5",
    "cross-env": "~7.0.2",
    "express": "~4.17.1",
    "helmet": "~3.22.0",
    "http-errors": "~1.7.2",
    "johnny-five": "~1.4.0",
    "jquery": "~3.5.0",
    "morgan": "~1.10.0",
    "muicss": "~0.10.2",
    "node-record-lpcm16": "~1.0.1",
    "pug": "~2.0.3",
    "raspi-io": "~11.0.0",
    "raspicam": "~0.2.14",
    "socket.io": "~2.3.0",
    "winston": "~3.2.1",
    "winston-daily-rotate-file": "~4.4.2"
  },
  "devDependencies": {
    "eslint": "~6.8.0",
    "eslint-config-airbnb-base": "~14.1.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-import": "~2.20.2",
    "eslint-plugin-prettier": "~3.1.3",
    "jsdoc": "~3.6.4",
    "prettier": "~2.0.5"
  }
}
